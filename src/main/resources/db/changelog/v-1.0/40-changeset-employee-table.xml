                     path="40-drop-employee-table.sql"/>
        </rollback>
    </changeSet>
    
    <changeSet id="1.10" author="den">
        <insert tableName="employee">
            <column name="position" value="Administrator"></column>
            <column name="contact_details_id" value="1"></column>
        </insert>
    </changeSet>

</databaseChangeLog>